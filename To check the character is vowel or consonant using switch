#include <stdio.h>

int main()
{
    char z;
    printf("Enter the alphabet:");
    scanf("%s",&z);
    switch(z)
    {
        case'a':
        case'A':
        case'e':
        case'E':
        case'i':
        case'I':
        case'o':
        case'O':
        case'u':
        case'U':
        printf("It is a vowel");
        break;
        default:
        printf("It is a consonant");
    }
   return 0;
}
